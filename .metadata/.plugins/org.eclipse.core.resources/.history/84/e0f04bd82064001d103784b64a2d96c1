package java_basics;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		double num1, num2, sum;
		
		num1 = sc.nextDouble();
		num2 = sc.nextDouble();
		
		sum = num1 + num2;
		
		System.out.println("Soma = " + sum);
		
	}

}
